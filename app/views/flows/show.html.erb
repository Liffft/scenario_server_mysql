
<div class="container-fluid">
<p>
  <div class="page-header">
  <h1> <%= @flow.flow_name %></h1>
  <table>
    <tbody>
		<% @flow.scenarios.each do |scenario| %>
		<tr>
			<td>
				<a href="/features/<%= @flow[:feature_id] %>/flows/<%= scenario[:flow_id]%>/scenarios/<%=scenario[:id]%>"><%= scenario[:scenario_name] %></a>
			</td>
		<% end %>
	</tbody>
</table>
</p>
</div>